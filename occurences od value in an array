#include <iostream>
using namespace std;
int main (){
  int arr[]={1,2,4,5,4,5,4};
  int size=sizeof(arr)/sizeof(arr[0]);
  int elementTofind=4;
  int count=0;
  int occur;
  for(int i=0;i<size;i++){
   if( arr[i]==elementTofind)
    count++;
  }
  cout<<"element "<<elementTofind<<" occours "<<count<<" times "<<endl;
}
