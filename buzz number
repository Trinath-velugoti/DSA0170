#include <iostream>
using namespace std;

int main() {
    int num;
    cout << "Enter an integer:"<<endl;
    cin >> num;

    if (num % 7 == 0)
    {
        cout << num << " is a buzz number." << endl;
    }
  else 
  {
        cout << num << " is not a buzz number." << endl;
    }
    return 0;
}
