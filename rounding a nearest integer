#include<iostream>
#include<cmath>
using namespace std;
int main()
{
 float num;
  
  cout<<"Enter a floating number:"<<endl;
  cin>>num;
  
  int roundedFloor=floor(num);
  int roundedCeil=ceil(num);
  
  cout<<"Rounded to the nereast integer using floor: "<<roundedFloor<<endl;
  cout<<"Rounded to the nereast integer using ceil : "<<roundedCeil;
  
 return 0; 
}
